    "express-validator": "^7.2.1",
    "jest": "^29.7.0",
    "prisma": "^6.10.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "engines": {
    "node": "18.x"
  },
  "directories": {
    "test": "tests"
  },
