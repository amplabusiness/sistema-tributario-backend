    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.34",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/imap": "^0.8.35",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mailparser": "^3.4.4",
    "@types/multer": "^1.4.11",
